    public Tabla() {
        initComponents();
        String[] titulo = new String[]{"Nombre ","Código ","Producto","Cantidad ","Monto "};
        dtm.setColumnIdentifiers(titulo);
        tblDatos.setModel(dtm);
        
    }

    void agregar(){
    dtm.addRow(new Object[] {
         txtNombre.getText(),txtCodigo.getText(),txtProducto.getText(),txtCantidad.getText(),txtMonto.getText()});
    
    }
